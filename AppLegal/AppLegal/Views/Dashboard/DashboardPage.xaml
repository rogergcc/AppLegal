<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppLegal.Views.Dashboard.DashboardPage">
    <ContentPage.Content>
        <StackLayout>
            <ListView x:Name="Post_List" >
                <ListView.ItemTemplate >
                    <DataTemplate>
                        <TextCell Text="{Binding ZonaTrabajoId}" Detail="{Binding Descripcion}"
                                  TextColor="DarkBlue" DetailColor="Black"/>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>