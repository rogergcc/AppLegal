                <Label x:Name="NombreArchivo" FontSize="13" HorizontalOptions="Center" 
                       Margin="2">
                </Label>
                <Label x:Name="Nemonico" FontSize="14" Text="Nemonico: " HorizontalOptions="Center" 
                       Margin="2">
                </Label>

                <Label x:Name="SubTipoServicio" FontSize="12" Text="Tipo Contrato: " TextColor="Brown" 
                       HorizontalOptions="Center" Margin="2"> 
                </Label>

                <Label x:Name="Fecha" FontSize="12" TextColor="Brown" 
                       HorizontalOptions="Center" Margin="2">
                </Label>
                <!--    
                <Button Text="Aprobar" BackgroundColor="#1094a1" HeightRequest="35" TextColor="White"></Button>
                -->
            </StackLayout>
        </Frame>
